{
  "files.autoSave": "onFocusChange",
  "editor.fontFamily": "JetBrains Mono,WenQuanYi Zen Hei Mono",
  "editor.lineHeight": 24,
  "editor.fontSize": 14,
  "vim.useSystemClipboard": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-d>": true,
    "<C-q>": false,
    "<C-b>": false,
    "<C-f>": false,
    "<C-]>": false,
    "<C-w>": false,
    "<C-k>": false,
  },
  "vim.foldfix": true,
  "vim.hlsearch": false,
  "cmake.configureOnOpen": true,
  "explorer.confirmDelete": false,
  "editor.formatOnType": true,
  "clangd.arguments": [
    "--compile-commands-dir=${workspaceFolder}/build",
    "--completion-style=bundled",
    "--header-insertion=never",
    "--suggest-missing-includes",
    "--clang-tidy",
    "--cross-file-rename",
    "--fallback-style=Microsoft"
  ],
  "editor.smoothScrolling": true,
  "editor.formatOnPaste": true,
  "editor.semanticHighlighting.enabled": true,
  "files.insertFinalNewline": true,
  "workbench.list.smoothScrolling": true,
  "vim.leader": ";",
  "vim.easymotion": true,
  "files.autoGuessEncoding": true,
  "editor.rename.enablePreview": false,
  "cmake.mingwSearchDirs": [
    "C:\\mingw64"
  ],
  "cmake.generator": "",
  "cmake.enabledOutputParsers": [
    "cmake",
    "gcc"
  ],
  "workbench.editor.enablePreview": false,
  "search.smartCase": true,
  "search.quickOpen.includeSymbols": true,
"todo-tree.tree.showScanModeButton": false,
  "git.ignoreLegacyWarning": true,
  "search.maintainFileSearchCache": true,
  "search.showLineNumbers": true,
  "editor.dragAndDrop": false,
  "editor.renderLineHighlight": "all",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorSurroundingLines": 10,
  "editor.formatOnSaveMode": "modifications",
  "editor.formatOnSave": false,
  "diffEditor.codeLens": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": true,
  "files.trimFinalNewlines": true,
  "workbench.tree.indent": 12,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.editor.highlightModifiedTabs": true,
  "window.restoreWindows": "none",
  "window.dialogStyle": "custom",
  "editor.suggest.showStatusBar": true,
  "wiki.basePath": "C:\\notes",
  "wiki.baseRootFile": "index.md",
  "editor.snippetSuggestions": "top",
  "emmet.showAbbreviationSuggestions": false,
  "editor.glyphMargin": false,
  "vim.smartRelativeLine": false,
  "workbench.editor.limit.value": 6,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.enabled": true,
  "workbench.sideBar.location": "right",
  "[cpp]": {
    "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
  },
  "clangd.onConfigChanged": "restart",
  "leetcode.endpoint": "leetcode-cn",
  "leetcode.workspaceFolder": "/home/duoml/.leetcode",
  "leetcode.defaultLanguage": "cpp",
  "C_Cpp.intelliSenseEngine": "Disabled",
  "leetcode.hint.configWebviewMarkdown": false,
  "leetcode.hint.commentDescription": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "leetcode.hint.commandShortcut": false,
  "C_Cpp.autocomplete": "Disabled",
  "clangd.path": "clangd-12",
  "C_Cpp.formatting": "Disabled",
  "C_Cpp.errorSquiggles": "Disabled",
  "clangd.checkUpdates": true,
  "C_Cpp.suggestSnippets": false,
  "python.analysis.completeFunctionParens": true,
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
"editor.wordWrap": "off",
  "editor.inlineSuggest.mode": "prefix",
  "editor.suggestSelection": "first",
  "todohighlight.isEnable": true,
  "git.countBadge": "off",
  "git.enableSmartCommit": true,
  "git.ignoreSubmodules": true,
  "git.untrackedChanges": "separate",
  "git.detectSubmodules": false,
  "security.workspace.trust.untrustedFiles": "open",
  "remote.SSH.remotePlatform": {
    "192.168.232.128": "linux",
    "192.168.232.136": "linux"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "workbench.iconTheme": "material-icon-theme",
  "terminal.explorerKind": "external",
  "workbench.experimental.layoutControl.enabled": true,
  "workbench.colorTheme": "Material Theme",
}
